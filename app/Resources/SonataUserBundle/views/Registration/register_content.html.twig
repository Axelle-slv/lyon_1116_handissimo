{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/login.css') }}">
{% endblock %}
 {% block test %}
     <div class=" panel-succes">
         <div class="title-block">
             <h2 class="login-title register">{{ 'title_user_registration'|trans({}, 'SonataUserBundle') }}</h2>
         </div>
         <div class="panel-body">
             <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register form-horizontal">

                 {{ form(form.username,  {'attr': {'class': 'form-control2'}}) }}
                 {{ form(form.email,  {'attr': {'class': 'mail'}}) }}
                 {{ form_widget(form.plainPassword.first, {'attr': {'class': 'form-control2'}}) }}
                 {{ form_widget(form.plainPassword.second, {'attr': {'class': 'form-control2'}}) }}

                 <div class="form-actions">
                     <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="btn btn-success pull-right" />
                 </div>
             </form>
         </div>
     </div>
 {% endblock %}