{% extends 'base.html.twig' %}

{% block body %}
    <h1>Disabilitytype</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ disabilityType.id }}</td>
            </tr>
            <tr>
                <th>Disabilityname</th>
                <td>{{ disabilityType.disabilityName }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('disabilitytypes_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('disabilitytypes_edit', { 'id': disabilityType.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
