{% extends 'base.html.twig' %}
{% block title %}{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/map.css') }}">
{% endblock %}
{% block body %}
    <!--
    <div id="floating-panel">
        <input id="address" type="textbox" value="Sydney, NSW">
        <input id="submit" type="button" value="Geocode">
    </div>
    -->

    <div id="map"></div>
    <ul>
        {% set newArray = [] %}
        {% for resultat in result %}
            {% if resultat not in newArray  %}
                <li>{{ resultat.name }}</li>
                {% set newArray = newArray|merge([resultat]) %}
            {% endif %}
        {% endfor %}
    </ul>

{% endblock %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset ('js/googlemap.js')}}"></script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAT1ybqTsqE0Nzit6xL7PfZWcgnLmThfXc&callback=initMap">
    </script>
{% endblock %}