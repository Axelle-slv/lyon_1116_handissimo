{% extends 'base.html.twig' %}

{% block body %}
    {{ form_start(form) }}
        {{ form_errors(form) }}
        {{ form_row(form.search) }}
            <ul id="autocomplete"></ul>
        {{ form_row(form.age) }}
            <ul id="age"></ul>
        {{ form_row(form.postal) }}
            <ul id="city"></ul>
    <input type="submit" value="Create" />

    {{ form_end(form) }}

{% endblock %}

{% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script src="{{ asset('js/essai2.js') }}"></script>
    <script src="{{ asset('js/searchcity.js') }}"></script>
{% endblock %}
